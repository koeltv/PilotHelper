{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/assets/silent-check-sso.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "navigationUrls": [
    "!/grafana",
    "!/grafana/**",
    "!/data",
    "!/data/**",
    "!/flight-plan",
    "!/flight-plan/**",
    "!/planning-tools",
    "!/planning-tools/**",
    "!/auth",
    "!/auth/**",
    "/**"
  ]
}
